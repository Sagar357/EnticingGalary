<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title</title>
    <link href="https://24wallpapers.com/Content/Admin/css/styles.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" crossorigin="anonymous" />
    <link rel="icon" type="image/x-icon" href="https://24wallpapers.com/favicon.ico" />
    <link href="~/Content/Admin/css/sweetalert.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Admin/summernote/summernote-bs4.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/js/all.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/feather-icons/4.24.1/feather.min.js" crossorigin="anonymous"></script>

    @*
        Chnage Log:
        * code ,name ,description ,date
        * ====================================================================================================
           #CC01 ,Sagar Srivastava ,Redirect Link,19-12-2020
    *@

</head>
<body class="nav-fixed">
    <nav class="topnav navbar navbar-expand shadow navbar-light bg-white" id="sidenavAccordion">
        <a class="navbar-brand d-none d-sm-block" href="@Url.Action("Index","Admin")">
            24Wallpapers

        </a><button class="btn btn-icon btn-transparent-dark order-1 order-lg-0 mr-lg-2" id="sidebarToggle" href="#"><i data-feather="menu"></i></button>
        <ul class="navbar-nav align-items-center ml-auto">
            <li class="nav-item dropdown no-caret mr-3 dropdown-user">
                <a class="btn btn-icon btn-transparent-dark dropdown-toggle" id="navbarDropdownUserImage" href="javascript:void(0);" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">

                    <img src="~/Content/web/images/logo1.png" class="img-fluid" />
                </a>
                <div class="dropdown-menu dropdown-menu-right border-0 shadow animated--fade-in-up" aria-labelledby="navbarDropdownUserImage">
                    <div class="dropdown-header d-flex align-items-center">
                        <div class="dropdown-user-details">
                            <div class="dropdown-user-details-name">Admin</div>

                        </div>
                    </div>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="@Url.Action("LogOff","AdminLogin")">
                        <div class="dropdown-item-icon"><i data-feather="log-out"></i></div>
                        Logout
                    </a>
                </div>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sidenav shadow-right sidenav-light">
                <div class="sidenav-menu">
                    <div class="nav accordion" id="accordionSidenav">
                        <div class="sidenav-menu-heading">Main</div>
                        <a class="nav-link collapsed" href="@Url.Action("Index","Admin")">
                            <div class="nav-link-icon"><i data-feather="activity"></i></div>
                            Dashboards
                        </a>
                        <div class="sidenav-menu-heading">Activity</div>
                        <a class="nav-link collapsed" href="@Url.Action("WallpaperHomePageContent","Admin")">
                            <div class="nav-link-icon"><i data-feather="package"></i></div>
                            Home Page Content
                        </a>
                        <a class="nav-link collapsed" href="@Url.Action("CategoryType","Admin")">
                            <div class="nav-link-icon"><i data-feather="package"></i></div>
                            Main Category
                        </a>
                        <a class="nav-link collapsed" href="@Url.Action("Category","Admin")">
                            <div class="nav-link-icon"><i data-feather="package"></i></div>
                            Sub Category
                        </a>
                        <a class="nav-link collapsed" href="@Url.Action("Wallpaper","Admin")">
                            <div class="nav-link-icon"><i data-feather="package"></i></div>
                            Wallpaper
                        </a>
                        <a class="nav-link collapsed" href="@Url.Action("Faqs","Admin")">
                            <div class="nav-link-icon"><i data-feather="package"></i></div>
                            Faq
                        </a>
                        <!-- #CC01 start -->

                        <a class="nav-link collapsed" href="@Url.Action("Index","Redirect")">
                            <div class="nav-link-icon"><i data-feather="package"></i></div>
                            Redirect Link
                        </a>

                        <!-- #CC01 end -->
                        <div class="sidenav-menu-heading">Support</div>

                        <a class="nav-link collapsed" href="@Url.Action("CustomerQuery","Admin")">
                            <div class="nav-link-icon"><i data-feather="layout"></i></div>
                            Customer Query
                        </a>
                        <div class="sidenav-menu-heading">Account</div>
                        <a class="nav-link collapsed" href="@Url.Action("LogOff","AdminLogin")">
                            <div class="nav-link-icon"><i data-feather="log-out"></i></div>
                            Logout
                        </a>

                    </div>
                </div>
                <div class="sidenav-footer">
                    <div class="sidenav-footer-content">
                        <div class="sidenav-footer-subtitle">Logged in as:</div>
                        <div class="sidenav-footer-title">24Wallpapers </div>
                    </div>
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                @RenderBody()
            </main>
            <footer class="footer mt-auto footer-light">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6 small">Copyright &#xA9; 2020 24wallpapers.com</div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="https://24wallpapers.com/Content/Admin/js/scripts.js"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js" crossorigin="anonymous"></script>
    <script src="https://24wallpapers.com/Content/Admin/assets/demo/datatables-demo.js"></script>
    <script src="https://24wallpapers.com/Content/Admin/js/sb-customizer.js"></script>

    <script src="https://24wallpapers.com/Content/Admin/js/sweetalert.js" type="text/javascript"></script>
    <script src="~/Content/Admin/summernote/summernote-bs4.min.js"></script>


    @RenderSection("deletescript", required: false)

    <script>
        $('#example1').DataTable();
    </script>

    <script>
        $(function () {
            // Summernote
            $('.summernote').summernote();
        })
    </script>

</body>
</html>
